{
    "servers": {
      "eden-context-window": {
        "name": "Eden Context Window MCP",
        "command": "node",
        "args": ["eden-context-window-mcp/src/index.js"],
        "port": 3001,
        "enabled": true,
        "auto_start": true,
        "dependencies": [],
        "description": "Context window management and symbolic memory",
        "category": "memory"
      },
      "eden-llm-context": {
        "name": "Eden LLM Context MCP",
        "command": "node",
        "args": ["eden-llm-context/src/index.js"],
        "port": 3002,
        "enabled": true,
        "auto_start": true,
        "dependencies": [],
        "description": "LLM context management and processing",
        "category": "ai"
      },
      "eden-mcp-forge": {
        "name": "Eden MCP Forge",
        "command": "node",
        "args": ["eden-mcp-forge/dist/cli.js"],
        "port": 3003,
        "enabled": true,
        "auto_start": false,
        "dependencies": [],
        "description": "MCP server development and forging tools",
        "category": "development"
      },
      "edenos-mcp-server": {
        "name": "EdenOS MCP Server",
        "command": "python",
        "args": ["edenos_mcp_server_bundle.py"],
        "port": 3004,
        "enabled": true,
        "auto_start": false,
        "dependencies": [],
        "description": "EdenOS filesystem and system operations",
        "category": "system"
      },
      "browser-mcp-bridge": {
        "name": "Browser MCP Bridge",
        "command": "node",
        "args": ["browser-mcp-bridge.js"],
        "port": 3005,
        "enabled": true,
        "auto_start": false,
        "dependencies": [],
        "description": "Browser automation and web interaction",
        "category": "browser"
      },
      "memory-lane-server": {
        "name": "Memory Lane Server",
        "command": "node",
        "args": ["memory-lane-server.js"],
        "port": 3006,
        "enabled": false,
        "auto_start": false,
        "dependencies": [],
        "description": "Memory lane and historical data access",
        "category": "memory"
      },
      "sequential-thinking": {
        "name": "Sequential Thinking MCP",
        "command": "node",
        "args": ["sequential_thinking/src/index.js"],
        "port": 3007,
        "enabled": false,
        "auto_start": false,
        "dependencies": [],
        "description": "Sequential thinking and problem decomposition",
        "category": "ai"
      },
      "universal-file-reader": {
        "name": "Universal File Reader",
        "command": "node",
        "args": ["universal-file-reader/src/index.js"],
        "port": 3008,
        "enabled": false,
        "auto_start": false,
        "dependencies": [],
        "description": "Universal file reading and format conversion",
        "category": "filesystem"
      }
    },
    "hub_settings": {
      "auto_restart": true,
      "health_check_interval": 30,
      "max_retries": 3,
      "log_level": "INFO",
      "startup_order": [
        "eden-context-window",
        "eden-llm-context",
        "eden-mcp-forge",
        "edenos-mcp-server",
        "browser-mcp-bridge"
      ],
      "server_groups": {
        "core": ["eden-context-window", "eden-llm-context"],
        "development": ["eden-mcp-forge"],
        "system": ["edenos-mcp-server"],
        "browser": ["browser-mcp-bridge"],
        "memory": ["memory-lane-server"],
        "ai": ["sequential-thinking"],
        "filesystem": ["universal-file-reader"]
      }
    }
  }