        "http://localhost:8083/mcp",
        "-H",
        "Content-Type: application/json",
        "-d",
        "{\"jsonrpc\":\"2.0\",\"id\":1,\"method\":\"tools/list\",\"params\":{}}"
      ],
      "env": {
